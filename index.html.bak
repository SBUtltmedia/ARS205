
<html>
  <head>
    <script src="https://cdn.jsdelivr.net/gh/aframevr/aframe@bd6bf821eda2522f84a1dc2b6d78674dfcd06e4a/dist/aframe-master.min.js"></script>
<script>
  let files=[
  "10_29_2025.glb",
"Isaac New.glb",
"Kai_solomon_scaniverse.glb",
"Scaniverse 2025-10-28 152539.glb"
]
AFRAME.registerComponent('init', {
  schema: {},
  init: init,
  update: function () {},
  tick: function () {},
  remove: function () {},
  pause: function () {},
  play: function () {}
});


function init(){


let scene= document.querySelector('a-scene');
let assets=document.querySelector("a-assets");
for (fileId in files)
{
let asset = document.createElement("a-asset-item")
asset.setAttribute("id",`id${fileId}`)
asset.setAttribute("src",`./${files[fileId]}`)
assets.appendChild(asset);
let entity = document.createElement("a-entity")
entity.setAttribute("gltf-model",`#id${fileId}`)
entity.setAttribute("position",`${fileId*4} 0 -5`)
scene.appendChild(entity);

}


}


</script>
  </head>
  <body>
    <a-scene init>
    <a-assets>
    </a-assets>


    </a-scene>
  </body>
</html>